<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %>
      </h1>
    </section>
  </aside>
</div>
<p>Name: <%= @user.name %> <br>Email: <%= @user.email %> <br>Created At: <%= @user.created_at %> <br> 
Updated At: <%= @user.updated_at %> <br>Current Time: <%= Time.now %></p>